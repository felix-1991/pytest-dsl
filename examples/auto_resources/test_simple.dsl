@name: "简单自动导入测试"
@description: "测试自动导入的简单关键字"
@author: "测试团队"
@date: "2024-01-15"

[打印], 内容: "开始简单测试"

# 使用简单问候关键字
问候结果 = [简单问候], 姓名: "张三"
[打印], 内容: "问候结果: ${问候结果}"

# 使用生成测试数据关键字
用户数据 = [生成测试数据], 数据类型: "用户", 数量: 2
订单数据 = [生成测试数据], 数据类型: "订单"
[打印], 内容: "用户数据: ${用户数据}"
[打印], 内容: "订单数据: ${订单数据}"

# 使用验证测试结果关键字
验证1 = [验证测试结果], 实际值: "通过", 预期值: "通过", 测试名称: "成功测试"
验证2 = [验证测试结果], 实际值: "失败", 预期值: "通过", 测试名称: "失败测试"

[打印], 内容: "验证1结果: ${验证1}"
[打印], 内容: "验证2结果: ${验证2}"

# 使用内置关键字进行数据比较
[数据比较], 实际值: ${验证1}, 预期值: "验证通过", 消息: "验证1应该通过"

# 使用简单计算关键字
加法结果 = [简单计算], 数字1: 15, 数字2: 25, 操作: "加法"
减法结果 = [简单计算], 数字1: 50, 数字2: 20, 操作: "减法"
乘法结果 = [简单计算], 数字1: 6, 数字2: 7, 操作: "乘法"

[打印], 内容: "加法结果: ${加法结果}"
[打印], 内容: "减法结果: ${减法结果}"
[打印], 内容: "乘法结果: ${乘法结果}"

# 验证计算结果
[数据比较], 实际值: ${加法结果}, 预期值: 40, 消息: "加法计算错误"
[数据比较], 实际值: ${减法结果}, 预期值: 30, 消息: "减法计算错误"
[数据比较], 实际值: ${乘法结果}, 预期值: 42, 消息: "乘法计算错误"

[打印], 内容: "简单测试完成！"

teardown do
    [打印], 内容: "执行简单测试清理"
    清理结果 = [清理测试环境], 环境类型: "简单测试"
    [打印], 内容: "清理完成: ${清理结果}"
end 